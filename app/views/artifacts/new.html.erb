<%= simple_form_for(@artifact) do |f| %>
    <%= f.input :name %>
    <%= f.input :artist %>
    <%= f.input :year %>
    <%= f.input :description %>
    <%= f.input :starting_value %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.button :submit, class: "btn btn-primary" %>
  <% end %>

  <%= link_to "Go back", artifacts_path  %>
